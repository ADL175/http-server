#!/Users/davidlim/Desktop/Project/401_python/wk2d1/http-server/HTTP_SERVER_ENV/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'http-server','console_scripts','test_servers'
__requires__ = 'http-server'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('http-server', 'console_scripts', 'test_servers')()
    )
